<p id="notice"><%= notice %></p>
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Listado de Editoriales</h3>
  </div>
  <div class="panel-body">

    <table class="table">
  <thead>
    <tr>
      <th>Rut</th>
      <th>Razon Social</th>
      <th>Direccion</th>
      <th>Email de Contacto</th>
      <th>Nombre de Contacto</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @editorials.each do |editorial| %>
      <tr>
        <td><%= editorial.editorial_rut %></td>
        <td><%= editorial.editorial_razon_social %></td>
        <td><%= editorial.editorial_direccion %></td>
        <td><%= editorial.editorial_email_contacto %></td>
        <td><%= editorial.editorial_nombre_contacto %></td>
        <td><%= editorial.editorial_descripcion %></td>
        <td><%= link_to 'Detalle', editorial %></td>
        <td><%= link_to 'Modificar', edit_editorial_path(editorial) %></td>
        <td><%= link_to 'Eliminar', editorial, method: :delete, data: { confirm: 'Â¿Seguro que deseas eliminar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Editorial', new_editorial_path %>
  </div>
</div>



